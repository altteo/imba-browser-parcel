{
    "name": "imba-browser-parcel",
    "version": "1.0.0",
    "description": "boilerplate to start frontend project with Imba and Parcel",
    "main": "source",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "imbac --output source/scripts --target web imba && parcel build source/index.html --out-dir release",
        "devserv": "parcel source/index.html --out-dir .cache",
        "imba": "imbac --watch --output source/scripts --target web imba",
        "dev": "npm-run-all --parallel imba devserv"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:altteo/imba-browser-parcel.git"
    },
    "author": "Fedor Leschinsky",
    "license": "MIT",
    "devDependencies": {
        "imba": "^1.3.3",
        "npm-run-all": "^4.1.3",
        "parcel-bundler": "^1.9.7",
        "parcel-plugin-clean-dist": "^0.0.6"
    },
    "dependencies": {}
}
